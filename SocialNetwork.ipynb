    "# Initialize the social network\n",
    "network = SocialNetwork()\n",
    "\n",
    "# Define possible topics\n",
    "topics = ['tech', 'lifestyle', 'science']\n",
    "\n",
    "# Function to create random topic preferences\n",
    "def random_topic_preferences():\n",
    "    return random.sample(topics, random.randint(1, 3))\n",
    "\n",
    "# Create 100 agents with random roles and topic preferences\n",
    "agent_names = [f'Agent{i}' for i in range(1, 20)]\n",
    "for name in agent_names:\n",
    "    role = random.choices(population=['Influencer', 'Sharer', 'StandardUser'], weights=[0.2,0.2,0.6])\n",
    "    topic_preference = random_topic_preferences()\n",
    "    if role[0] == 'Influencer':\n",
    "        agent = Influencer(name, topic_preference=topic_preference)\n",
    "    elif role[0] == 'Sharer':\n",
    "        agent = Sharer(name, topic_preference=topic_preference)\n",
    "    else:\n",
    "        agent = StandardUser(name, topic_preference=topic_preference)\n",
    "    network.add_agent(agent)\n",
    "\n",
    "# Create random follower relationships\n",
    "for name in agent_names:\n",
    "    agent = network.agents_set[name]\n",
    "    if agent.type == 'Influencer':\n",
    "        num_followers = random.randint(4, 5)\n",
    "    elif agent.type == 'Sharer':\n",
    "        num_followers = random.randint(1, 2)\n",
    "    else:\n",
    "        num_followers = random.randint(0, 1)\n",
    "    followers = random.sample(agent_names, num_followers)\n",
    "    followers = [follower for follower in followers if follower != name]\n",
    "    network.create_followers(name, followers)\n",
    "\n",
    "print(\"Initial State:\")\n",
    "network.visualize_network(step=0)\n",
    "for agent_id, agent in network.agents_set.items():\n",
    "    print(f\"{agent_id} has {len(agent.followers)} followers\")"
   ]
